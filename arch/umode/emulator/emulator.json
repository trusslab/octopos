{
  "session_name": "OctopOS",
  "windows": [
    {
      "window_name": "OctopOS terminals",
      "layout": "tiled",
      "panes": [
	{
	   "shell_command":[
		"arch/umode/emulator/log_view -o"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -k"
	   ]
	},{
	   "shell_command":[
		"cd applications/socket_client",
		"while true; do ./socket_server; done"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -m"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -l"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -1"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -s"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -n"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -2"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -u"
	   ]
	},{
	   "shell_command":[
		"arch/umode/emulator/log_view -p"
	   ]
	},{
	   "shell_command":[
		"sudo arch/umode/pmu/pmu"
	   ]
	}
      ],
      "options_after": {
        "mouse": true
      }
    }
  ]
}
