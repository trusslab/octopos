{
  "session_name": "OctopOS",
  "windows": [
    {
      "window_name": "OctopOS terminals",
      "layout": "tiled",
      "panes": [
	{
	   "shell_command":[
		"cd os",
		"sleep 2",
		"./os"
	   ]
	},{
	   "shell_command":[
		"cd loader",
		"sleep 1",
		"./loader ../keyboard/keyboard.so"
	   ]
	},{
	   "shell_command":[
		"cd applications/socket_client",
		"while true; do ./socket_server; done"
	   ]
	},{
	   "shell_command":[
		"cd loader",
		"./loader ../arch/umode/mailbox/mailbox.so"
	   ]
	},{
	   "shell_command":[
		"cd loader",
		"sleep 1",
		"./loader ../serial_out/serial_out.so"
	   ]
	},{
	   "shell_command":[
		"cd runtime",
		"sleep 1",
		"while true; do ./runtime 1; done"
	   ]
	},{
	   "shell_command":[
		"cd loader",
		"sleep 1",
		"./loader ../storage/storage.so"
	   ]
	},{
		"shell_command": [
			"cd tpm",
			"sleep 1",
			"./tpm_monitor"
		]
	},{
	   "shell_command":[
		"cd network",
		"sleep 1",
		"sudo ./network"
	   ]
	},{
	   "shell_command":[
		"cd runtime",
		"sleep 1",
		"while true; do ./runtime 2; done"
	   ]
	},{
	   "shell_command":[
		"cd arch/umode/untrusted_linux",
		"sleep 1",
		"./linux ubda=OpenSuse-12.1-amd64-root_fs mem=128M",
		"test"
	   ]
	}
      ],
      "options_after": {
        "mouse": true
      }
    }
  ]
}
