{
  "session_name": "OctopOS",
  "windows": [
    {
      "window_name": "OctopOS terminals",
      "layout": "tiled",
      "panes": [
	{
	   "shell_command":[
		"cd os",
		"sleep 2",
		"./os"
	   ]
	},{
	   "shell_command":[
		"cd keyboard",
		"sleep 1",
		"./keyboard"
	   ]
	},{
	   "shell_command":[
		"cd applications/socket_client",
		"while true; do ./socket_server; done"
	   ]
	},{
	   "shell_command":[
		"cd arch/umode/mailbox",
		"./mailbox"
	   ]
	},{
	   "shell_command":[
		"cd serial_out",
		"sleep 1",
		"./serial_out"
	   ]
	},{
	   "shell_command":[
		"cd runtime",
		"sleep 1",
		"while true; do ./runtime 1; done"
	   ]
	},{
	   "shell_command":[
		"cd storage",
		"sleep 1",
		"./storage"
	   ]
	},{
	   "shell_command":[
		"cd network",
		"sleep 1",
		"sudo ./network"
	   ]
	},{
	   "shell_command":[
		"cd runtime",
		"sleep 1",
		"while true; do ./runtime 2; done"
	   ]
	},{
	   "shell_command":[
		"cd arch/umode/untrusted_linux",
		"sleep 1",
		"./linux ubda=CentOS6.x-AMD64-root_fs mem=128M",
		"test"
	   ]
	}
      ],
      "options_after": {
        "mouse": true
      }
    }
  ]
}
